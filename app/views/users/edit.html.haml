- title("Edit Profile")
#content2

  %h1 Edit Profile
  - @user.password = @user.password_confirmation = nil

  = error_messages_for :user
  - form_for :user, :url => users_path do |f|
    %p
      =@user.login

    = render :partial => "users/form", :locals => { :user => @user, :f => f }

    %p
      =submit_tag 'Edit'

